            <div class="row">
                <div class="col-8 p-2 align-self-center offset-2">

                    <form id="my-form"
                    action="https://formspree.io/mdbevagm"
                    method="POST"
                    >
                    <h4 class="mb-4 text-center">
                                Belafspraak plannen</h4>
                        <div class="form-group">
                            <label>Voor en achternaam</label>
                            <input type="text" name="Naam" class="form-control" placeholder="Voor en achternaam"> </div>
                        <div class="form-group"> <label>Emailadres</label>
                            <input type="text" name="e-mail" class="form-control" placeholder="E-mail"> </div>
                        <div class="form-group"> <label>Telefoonnummer</label>
                            <input type="text" name="telefoon" class="form-control" placeholder="Telefoonnummer"> </div>
                        <div class="form-group"> <label>Moment 1 van contact</label>
                            <input type="date" name="datum" class="form-control">
                        </div>
                        <div class="form-group">
                            <div class="input-group clockpicker">
                                <input type="text" name="Moment 1" class="form-control" value="09:30">
                                <span class="input-group-addon glyf">
                                    <span class="glyphicon glyphicon-time"></span>
                                </span>
                            </div>
                        </div>
                        <div class="form-group"> <label>Moment 2 van contact</label>
                            <input type="date" name="datum2" class="form-control">
                        </div>
                        <div class="form-group">
                            <div class="input-group clockpicker">
                                <input type="text" name="Moment 2" class="form-control" value="09:30">
                                <span class="input-group-addon glyf">
                                    <span class="glyphicon glyphicon-time"></span>
                                </span>
                            </div>
                        </div>
                        <br>
                        <br>
                        <p class="center" id="my-form-status"><b></b></p>

                    <button id="my-form-button" class="btn mt-4 btn-block btn-new p-2">Verzenden</button>

                    </form>
                </div>
            </div>
